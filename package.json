    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
