  "description": "API para o aplicativo imobili√°rio",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --exit-child --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:legacy": "vitest run",
    "test:unit": "jest --runInBand --testPathPattern=unit",
    "test:integration": "jest --runInBand --testPathPattern=integration|int",
    "test:migrations": "jest --runInBand --testPathPattern=migrations",
    "test:negotiations": "jest --runInBand --testPathPattern=negotiations",
    "migrate:up": "ts-node src/database/migrationRunner.ts up",
    "migrate:down": "ts-node src/database/migrationRunner.ts down",
    "migrate:status": "ts-node src/database/migrationRunner.ts status",
    "db:init": "ts-node src/database/init.ts"
  },
  "keywords": [],
  "author": "Ctrshift-Pm",
  "license": "MIT",
  "dependencies": {
    "@types/nodemailer": "^7.0.5",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.4.1",
    "express": "^4.19.2",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.0",
    "nodemailer": "^7.0.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
